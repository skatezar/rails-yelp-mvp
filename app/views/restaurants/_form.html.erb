
<div class="card">
  <%= form_for(restaurant) do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :address %>
      <%= f.text_field :address %>
    </div>
    <div>
      <%= f.label :category %>
      <%= f.text_field :category %>
    </div>
    <div>
      <%= f.label :phone_number %>
      <%= f.number_field :phone_number %>
    </div>
    <%= f.submit %>
  <% end %>
</div>
